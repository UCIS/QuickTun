#!/bin/sh
test -n "${IF_QT_REMOTE_ADDRESS}" || exit 0
test -z "${IF_QT_NO_PRECREATE}" || exit 0
if [ -x /sbin/ip ] && /sbin/ip tuntap 2>&1 >/dev/null; then
	if [ -n "${IF_QT_TUN_MODE}" ] && [ "${IF_QT_TUN_MODE}" = "1" ]; then
	        DEVTYPE="tun"
	else
	        DEVTYPE="tap"
	fi
	/sbin/ip tuntap del dev "${IFACE}" mode "${DEVTYPE}"
fi
